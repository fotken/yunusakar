{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "phone": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "customer"
    },
    "email": {
      "type": "email"
    },
    "userId": {
      "type": "string",
      "unique": true
    },
    "reservations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reservation.reservation",
      "mappedBy": "customer"
    },
    "expoPushToken": {
      "type": "string",
      "required": false
    }
  }
}
